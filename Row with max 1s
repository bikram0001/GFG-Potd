class Solution:
    def rowWithMax1s(self, arr):
        # code here
        m = len(arr)
        n = len(arr[0])
        result = -1
        i = 0
        j = n-1
        while(i<m and j>=0):
            if(arr[i][j]&1):
                j-=1
                result = i
            else:
                i+=1
        return result
