class Solution:
    def kthMissing(self, arr, k):
        # code here
        low = 0
        high = len(arr)-1
        while(low<=high):
            mid = low+(high-low)//2
            value = arr[mid]-mid+1
            if(arr[mid]-mid-1  < k):
                low = mid+1
            else:
                high = mid-1
        return k+low
